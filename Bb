    }
    public String getFrstIsoCurrCd() {
        return frstIsoCurrCd;
    }
    public void setFrstIsoCurrCd(String frstIsoCurrCd) {
        this.frstIsoCurrCd = frstIsoCurrCd;
    }
    public Double getContent() {
        return content;
    }
    public void setContent(Double content) {
        this.content = content;
    }
}
